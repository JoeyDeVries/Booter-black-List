set terminal postscript eps size 6.5,3.5 enhanced color font 'Arial,20' linewidth 2
set output "cosine_weighted.eps"
set datafile separator ","
#set style data points
set ylabel "Accuracy"
set xlabel "Threshold T"
set yrange [0:1]
set xrange [0:1]
set key left top
set style line 1 lt 1 lw 2 lc rgb '#4477AA' 
set style line 2 lt 3 lw 2 lc rgb '#CC6677' 
set style line 3 lt 5 lw 2 lc rgb '#117733' 
set style line 4 lt 1 lw 2 lc rgb '#AAAAAA'
set style rect fc rgb '#DDDDDD' fs solid 1.0 
set obj rect from 0.93, graph 0 to 0.98, graph 1
set arrow 1 from 0.98,0.5 to 0.94,0.5 ls 4
#set title "600 byte packet throughput"
plot 'cosine_weighted.dat' using 1:2 title 'CAR' ls 1 with lines, '' using 1:3 title columnheader(3) ls 2 with lines, '' using 1:4 title columnheader(4) ls 3 with lines
